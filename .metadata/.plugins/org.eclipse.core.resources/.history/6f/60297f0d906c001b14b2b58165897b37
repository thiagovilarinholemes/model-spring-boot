package com.api.repositories;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;

import com.api.dto.TutorialDTO;
import com.api.entities.Tutorial;

public interface TutorialRepository extends JpaRepository<TutorialDTO, Long>{
		
	List<TutorialDTO> findAllByOrderByNameAsc();
	
	List<TutorialDTO> findByPublished(Boolean published);
 
	List<TutorialDTO> findByTitleContaining(String title);

}
